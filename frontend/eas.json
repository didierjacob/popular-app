{
  "cli": {
    "version": ">= 3.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      },
      "node": "22.12.0"
    },
    "device": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "node": "22.12.0"
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "resourceClass": "m-medium"
      },
      "node": "22.12.0"
    }
  },
  "submit": {
    "production": {}
  }
}
